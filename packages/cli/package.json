{
  "name": "vibepity",
  "version": "0.1.0",
  "type": "module",
  "description": "ChatGPT OAuth-powered open-source AI assistant + coding agent",
  "license": "MIT",
  "bin": {
    "vibepity": "dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --external @vibepity/auth --external @vibepity/proxy --external @vibepity/workspace --external @vibepity/providers --external @vibepity/telegram --external @vibepity/budget --external @vibepity/skills --external @vibepity/core --external @vibepity/daemon --external grammy",
    "dev": "tsx src/index.ts",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@vibepity/auth": "workspace:*",
    "@vibepity/proxy": "workspace:*",
    "@vibepity/workspace": "workspace:*",
    "@vibepity/providers": "workspace:*",
    "@vibepity/telegram": "workspace:*",
    "@vibepity/budget": "workspace:*",
    "@vibepity/skills": "workspace:*",
    "@vibepity/core": "workspace:*",
    "@vibepity/daemon": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.4.0",
    "tsx": "^4.19.0",
    "typescript": "^5.8.0",
    "vitest": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vibepity/vibepity"
  },
  "keywords": [
    "ai",
    "chatgpt",
    "codex",
    "agent",
    "assistant",
    "cli",
    "oauth",
    "openclaw"
  ]
}
