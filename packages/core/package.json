{
  "name": "@vibepity/core",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@vibepity/auth": "workspace:*",
    "@vibepity/providers": "workspace:*",
    "@vibepity/workspace": "workspace:*",
    "@vibepity/budget": "workspace:*",
    "@vibepity/skills": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.4.0",
    "typescript": "^5.8.0",
    "vitest": "^3.0.0"
  }
}
